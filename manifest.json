{
	"manifest_version": 2,
	"name": "txtToText",
	"version": "1.0",
	"description": "Replaces text lingo with full words",
	"icons": {
		"16": "txtToTextIcon.png"
	},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"]
		}
	],
	"permissions": [
		"https://*/",
		"http://*/",
		"tabs"
	]
}